@model IEnumerable<HotelManager.Core.ApplicationUser>

@{
    ViewData["Title"] = "Staff";
}

<h1>Staff</h1>

<p>
    <a asp-action="NewHotelStaff">New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FullName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FullName)
            </td>
            <td></td>
        </tr>
}
    </tbody>
</table>
